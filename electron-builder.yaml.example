appId: com.ksb.app

publish:
    provider: github
    token: YOUR_GITHUB_TOKEN

files:
    - build/**/*
    [
        "dist/**/*",
        "build/**/*",
        "node_modules/**/*",
        "package.json",
        "build/icon.*",
    ]

directories:
    buildResources: assets

icon: ./build/icon.ico
